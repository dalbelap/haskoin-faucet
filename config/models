User 
    ip Text maxlen=200
    withdrawTime UTCTime 
    withdrawAmount Word64
    UniqueIp ip

Audit
    ip Text maxlen=200
    event Text maxlen=200
    message Text
    withdrawAmount Word64
    created UTCTime

Nonce
    token Text
    value Int
    UniqueToken token

